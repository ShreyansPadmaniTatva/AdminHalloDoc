<div class="modal-header">
    <h4 class="modal-title">@ViewBag.Action Level</h4>
    <button type="button" class="btn btn-danger btn-elevate btn-icon btn-sm" data-bs-dismiss="modal" aria-label="Close">
        <i class="fa fa-close"></i>
    </button>
</div>

<div class=" modal-body">
    <form id="formAddEditSave_UMS" data-ajax="true" data-ajax-method="post"
          data-ajax-success="OnSuccess(this.id,data, status, xhr);" data-ajax-failure="OnFailure(xhr, status, error);"
          asp-action="_Save" asp-controller="MST_Level">
        @Html.AntiForgeryToken()

        <div class=" form-group row">
            
            <div class="col-md-6">
                <label class="required  col-form-label ">Level Name</label>
                @*@Html.TextBoxFor(m => m.CategoryName, new { @class = "form-control", placeholder="Enter Category Name"})*@
            </div>
            <div class="col-md-6">
                <label class="col-form-label">Sequence</label>
            </div>
        </div>

        <div class="form-group row">
            <div class="col-md-12">
                <label class="col-form-label"> Level Description</label>
            </div>
        </div>
      
        <div class="form-group row">
            <div class="col-md-12">
            <label class="col-form-label "> Description</label>
            </div>
        </div>
  @*<div class="form-group row">
            <label class="col-md-4 col-form-label"> UserID</label>
            <div class="col-md-5">

                <input type="text" asp-for="UserID" class="form-control" />
                @Html.ValidationMessageFor(m => m.UserID, "", new { @class = "form-text text-danger" })
            </div>
        </div>*@
        <hr class="form-button-divider" />
        <div class="row">
            <div class="offset-md-4 col-md-8">
                <input type="submit" value="Save" class="btn btn-primary btn-sm js-btn-modal-form-submit" />
                <button type="button" class="btn btn-light btn-sm" data-bs-dismiss="modal">Cancel</button>
            </div>
        </div>
    </form>
</div>

