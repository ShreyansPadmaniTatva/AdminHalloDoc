    <div id="modal-placeholder"></div>
    <partial name="_Partial/_footer.cshtml" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <script src="~/js/signalr/dist/browser/signalr.js"></script>
